{
  "name": "web",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "// Next Environment": "----------------",
    "build": "next build",
    "start": "next start",
    "dev": "next dev --turbopack",
    "// Utility Commands": "----------------",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "validate": "pnpm lint && pnpm format:check && pnpm type-check",
    "faucet": "pnpm dubhe faucet --network",
    "account:gen": "pnpm dubhe generate-key --use-next-public",
    "check-balance": "pnpm dubhe check-balance --network",
    "// Production Environment": "----------------",
    "prod:localnet": "pnpm account:gen && pnpm faucet localnet",
    "prod:testnet": "pnpm account:gen && pnpm check-balance testnet",
    "prod:mainnet": "pnpm account:gen && pnpm check-balance mainnet"
  },
  "dependencies": {
    "@0xobelisk/sui-client": "^1.2.0-pre.78",
    "@0xobelisk/sui-cli": "^1.2.0-pre.78",
    "@0xobelisk/sui-common": "^1.2.0-pre.78",
    "contracts": "workspace:*",
    "@mysten/dapp-kit": "^0.15.2",
    "@mysten/enoki": "^0.6.2",
    "@mysten/sui": "^1.26.1",
    "@mysten/zksend": "^0.12.22",
    "@tanstack/react-query": "^5.71.10",
    "@workspace/ui": "workspace:*",
    "canvas-confetti": "^1.9.3",
    "dotenv": "^16.5.0",
    "jotai": "^1.7.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "0.456.0",
    "next": "^15.1.1",
    "next-themes": "^0.4.3",
    "phaser": "^3.87.0",
    "tweakpane": "^4.0.5",
    "webfontloader": "^1.6.28",
    "@ai-sdk/deepseek": "^0.2.14",
    "@ai-sdk/openai": "^1.3.16",
    "@ai-sdk/react": "^1.2.9",
    "ai": "^4.3.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@tweakpane/core": "^2.0.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webfontloader": "^1.6.38",
    "@workspace/eslint-config": "workspace:^",
    "@workspace/typescript-config": "workspace:*",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
